    </j.0:knows>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/Stefan">
    <ex:thinks>
      <rdf:Statement>
        <rdf:object>
          <rdf:Statement>
            <rdf:object rdf:resource="http://example.org/Father"/>
