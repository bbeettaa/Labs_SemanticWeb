        </rdf:subject>
      </rdf:Statement>
    </ex:knows>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/Ivan">
    <j.0:knows>
      <rdf:Statement>
        <rdf:object rdf:resource="http://example.org/Italy"/>
